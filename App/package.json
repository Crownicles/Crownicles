{
  "name": "Crownicles",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-dev": "pnpm run build:protobuf && react-native run-android --mode=developmentDebug --appId=com.crownicles.development",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:protobuf": "mkdir -p src/@types && pbjs -t static-module -p protobufs --force-number -o src/@types/protobufs-client.js ../RestWs/protobufs/from_client/**/*.proto && pbts -o src/@types/protobufs-client.d.ts src/@types/protobufs-client.js && pbjs -t static-module -p protobufs --force-number -o src/@types/protobufs-server.js ../RestWs/protobufs/from_server/**/*.proto && pbts -o src/@types/protobufs-server.d.ts src/@types/protobufs-server.js && sed -i 's/export interface \\([A-Za-z0-9]\\+\\) {/export interface \\1 extends FromClientPacket {/g' src/@types/protobufs-client.d.ts && sed -i 's/export interface \\([A-Za-z0-9]\\+\\) {/export interface \\1 extends FromServerPacket {/g' src/@types/protobufs-server.d.ts && sed -i '1s/^/export interface FromClientPacket {}\\n/' src/@types/protobufs-client.d.ts && sed -i '1s/^/export interface FromServerPacket {}\\n/' src/@types/protobufs-server.d.ts",
    "android-logs": "react-native log-android",
    "ios-logs": "react-native log-ios"
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.80.1",
    "@react-navigation/native": "^7.1.14",
    "buffer": "^6.0.3",
    "expo": "~51.0.39",
    "react": "19.1.0",
    "react-native": "0.80.1",
    "react-native-config": "^1.5.5",
    "react-native-inappbrowser-reborn": "^3.7.0",
    "react-native-keychain": "^10.0.0",
    "react-native-popup-menu": "^0.18.0",
    "react-native-uuid": "^2.0.3",
    "react-native-vector-icons": "^10.2.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@protobuf-ts/plugin": "^2.11.1",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.1",
    "@react-native/eslint-config": "0.80.1",
    "@react-native/metro-config": "0.80.1",
    "@react-native/typescript-config": "0.80.1",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "9.30.1",
    "eslint-plugin-jsdoc": "^51.3.4",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "protobufjs": "^7.5.3",
    "protobufjs-cli": "^1.1.3",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912"
}
