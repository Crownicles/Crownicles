{
  "name": "crownicles_discord_middleware",
  "version": "5.1.9.a",
  "description": "Discord middleware package of Crownicles",
  "main": "src/index.js",
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f",
  "engines": {
    "node": ">=20.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "start": "pnpm run interface && pnpm run tsc && node dist/Discord/src/index.js",
    "setup": "pnpm i",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "cd": "cd",
    "eslint": "eslint src",
    "eslintFix": "eslint src --fix",
    "tsc": "tsc",
    "dockerStart": "node dist/Discord/src/index.js",
    "interface": "i18next-resources-for-ts interface -i ../Lang/en -o ./src/@types/resources.d.ts"
  },
  "dependencies": {
    "@discordjs/builders": "1.11.2",
    "@types/express": "5.0.3",
    "@types/node": "24.0.8",
    "discord-api-types": "0.38.13",
    "discord.js": "14.21.0",
    "express": "5.1.0",
    "i18next": "25.3.0",
    "i18next-resources-for-ts": "1.6.0",
    "mariadb": "3.0.2",
    "moment": "2.30.1",
    "mqtt": "5.13.1",
    "prom-client": "15.1.3",
    "random-js": "2.1.0",
    "sequelize": "6.37.7",
    "source-map-support": "0.5.21",
    "toml": "3.0.0",
    "topgg-autoposter": "2.0.2",
    "triple-beam": "1.4.1",
    "typescript": "5.8.3",
    "umzug": "3.8.2",
    "winston": "3.17.0",
    "winston-daily-rotate-file": "5.0.0",
    "winston-loki": "6.1.3"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "5.1.0",
    "@types/triple-beam": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "8.35.1",
    "@typescript-eslint/parser": "8.35.1",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.30.0",
    "eslint-plugin-jsdoc": "51.3.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.1",
    "vitest": "3.2.4"
  }
