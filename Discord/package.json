{
  "name": "draftbot_discord_middleware",
  "version": "5.0.0.g",
  "description": "Discord middleware package of DraftBot",
  "main": "src/index.js",
  "packageManager": "yarn@4.8.1",
  "engines": {
    "node": ">=22.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "start": "yarn interface && yarn run tsc && node dist/Discord/src/index.js",
    "cd": "cd",
    "eslint": "eslint src",
    "eslintFix": "eslint src --fix",
    "tsc": "tsc",
    "yarnInstall": "yarn install",
    "dockerStart": "node dist/Discord/src/index.js",
    "interface": "i18next-resources-for-ts interface -i ../Lang/en -o ./src/@types/resources.d.ts"
  },
  "dependencies": {
    "@discordjs/builders": "1.10.1",
    "@types/express": "5.0.1",
    "@types/i18next": "13.0.0",
    "@types/node": "22.13.17",
    "@types/winston": "2.4.4",
    "discord-api-types": "0.37.119",
    "discord.js": "14.18.0",
    "express": "5.1.0",
    "i18next": "24.2.3",
    "i18next-resources-for-ts": "1.5.0",
    "mariadb": "3.0.2",
    "moment": "2.30.1",
    "mqtt": "5.10.4",
    "prom-client": "15.1.3",
    "random-js": "2.1.0",
    "sequelize": "6.37.7",
    "source-map-support": "0.5.21",
    "toml": "3.0.0",
    "topgg-autoposter": "2.0.2",
    "triple-beam": "1.4.1",
    "umzug": "3.8.2",
    "winston": "3.17.0",
    "winston-daily-rotate-file": "5.0.0",
    "winston-loki": "6.1.3"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "4.2.0",
    "@types/triple-beam": "1",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "@vitest/coverage-v8": "3.1.2",
    "eslint": "9.23.0",
    "eslint-plugin-jsdoc": "50.6.9",
    "typescript": "5.6.3",
    "typescript-eslint": "8.29.0",
    "vitest": "3.1.2"
  }
}
