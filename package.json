		"link-module-alias": "1.2.0",
		"mariadb": "3.0.2",
		"source-map-support": "0.5.21",
		"moment": "2.29.4",
		"node-ipc": "10.1.0",
		"random-js": "2.1.0",
		"sequelize": "6.32.1",
