		"@types/node-fetch": "2.6.4",
		"@types/node-ipc": "9.2.0",
		"@types/validator": "13.7.17",
		"@types/ws": "8.5.5",
		"eslint": "8.46.0",
		"eslint-config-google": "0.14.0",
		"jest": "29.6.2",
		"jshint": "2.13.6",
		"ts-jest": "29.1.1"
	}
